{
    "defaultExtension": "py",
    "path": {
        "*.htm": {
            "spaceUnits": 4
        },
        "*.html": {
            "spaceUnits": 2,
            "themes.theme": "dark-theme"
        },
        "*.css": {
            "spaceUnits": 4,
            "themes.theme": "dark-theme"
        },
        "*.py": {
            "spaceUnits": 4,
            "themes.theme": "dark-theme"
        },
        "*.txt": {
            "spaceUnits": 2,
            "wordWrap": true
        }
    },
    "wordWrap": true,
    "spaceUnits": 4,
    "useTabChar": false
}